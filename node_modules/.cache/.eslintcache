essedMessages":"87","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"88","messages":"89","suppressedMessages":"90","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"91","messages":"92","suppressedMessages":"93","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"94","messages":"95","suppressedMessages":"96","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"97","messages":"98","suppressedMessages":"99","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"100","messages":"101","suppressedMessages":"102","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"103","messages":"104","suppressedMessages":"105","errorCount":0,"fatalErrorCount":0,"warningCount":3,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"106","messages":"107","suppressedMessages":"108","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/index.js",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/App.js",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/reportWebVitals.js",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/context/auth.context.js",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/Videogames.jsx",["109","110","111"],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/Home.jsx",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/user/Collections.jsx",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/user/ProfileEdit.jsx",["112"],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/VideogamesDetails.jsx",["113","114"],[],"import React, { useContext, useEffect, useState } from \"react\";\nimport { Link, useNavigate, useParams } from \"react-router-dom\";\nimport PulseLoader from \"react-spinners/PulseLoader\";\nimport { AuthContext } from '../context/auth.context'\nimport {\n  listGamesDbService,\n  listGamesDetailsService,\n  listGamesTrailersService,\n} from \"../services/games.services\";\n\n// Hemos instalado Markup de Interweave con npm i interweave. Convierte strings de html en strings jsx.\nimport { Markup } from \"interweave\";\nimport Comments from \"../components/Comments\";\n\nfunction VideogamesDetails() {\n  const { isLoggedIn } = useContext(AuthContext)\n  const { id } = useParams();\n  const navigate = useNavigate();\n\n  // Llamar a la API\n  //! 1. Estados\n  const [gameDetails, setGameDetails] = useState(null);\n  // TODO ---> Estado para los trailers\n  const [gameTrailer, setGameTrailers] = useState(null);\n  const [status, setStatus] = useState(\"isOwned\");\n\n  const handleStatusChange = (event) => {\n    setStatus(event.target.value);\n    console.log(event.target.value);\n  };\n\n  const handleSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const dbDetails = {\n        gameApiId: id,\n        title: gameDetails.name,\n        status,\n      };\n\n      await listGamesDbService(id, dbDetails);\n      navigate(`/videogames/${id}/collections`);\n    } catch (error) {\n      navigate(\"/error\");\n    }\n  };\n\n  //! 2. Acceder al componentDidMount\n  // TODO ---> componentDidMount para los trailers debajo\n  useEffect(() => {\n    getVideogamesDetails();\n    getVideogamesTrailers();\n  }, []);\n\n  //! 3. Funci√≥n que llama a la API y se comunica con componentDidMount\n  const getVideogamesDetails = async () => {\n    try {\n      // 1. Llamada a la API\n      const response = await listGamesDetailsService(id);\n      console.log(\"Games Details\", response.data);\n\n      // 2. Actualizamos el estado con la respuesta de la API.\n      setGameDetails(response.data);\n      console.log(\"response.data\", response.data);\n    } catch (error) {\n      navigate(\"/error\");\n    }\n  };\n\n  // TODO ---> Funci√≥n de comunicaci√≥n con la API para los trailers\n  const getVideogamesTrailers = async () => {\n    try {\n      const response = await listGamesTrailersService(id);\n      setGameTrailers(response.data);\n      console.log(\"Trailers and bullshit:\", response.data);\n    } catch (error) {\n      navigate(\"/error\");\n    }\n  };\n\n  //! 4. Crear efecto de Loading.\n  if (gameDetails === null) {\n    return (\n      <>\n        <h4>Cargando...</h4>\n        <PulseLoader color={\"rgb(0,0,0)\"} />\n      </>\n    );\n    // TODO ---> Lo mismo pero para los trailers\n  } else if (gameTrailer === null) {\n    return (\n      <>\n        <h4>Cargando...</h4>\n        <PulseLoader color={\"rgb(0,0,0)\"} />\n      </>\n    );\n  }\n\n  return (\n    <div>\n      {console.log(\"Informaci√≥n de trailer:\", gameTrailer)}\n      <h3>Detalles del Videojuego</h3>\n      <hr />\n\n      <div key={gameDetails.id}>\n        {/* Imagen del juego */}\n        <img src={gameDetails.background_image} width={900} alt=\"Cover\" />\n        {/* <img\n          src={gameDetails.background_image_additional}\n          width={900}\n          alt=\"Cover\"\n        /> */}\n\n        {/* Formulario para a√±adir el juego a colecciones (conexi√≥n con User.Model) */}\n        { isLoggedIn === true &&\n        <form onSubmit={handleSubmit}>\n          <br />\n          <select name=\"select\" onChange={handleStatusChange}>\n            <option value={\"isOwned\"}>Owned games</option>\n            <option value={\"isWished\"}>Wished games</option>\n            <option value={\"isFinished\"}>Finished games</option>\n            <option value={\"isPlaying\"}>Currently playing games</option>\n          </select>\n          <br /> <br />\n          <button type=\"submit\">Submit</button>\n        </form> }\n\n        {/* Nombre del juego */}\n        <h1>{gameDetails.name}</h1>\n\n        {/* Nombres alternativos del juego que vienen en array */}\n        {gameDetails.alternative_names.length !== 0 &&\n          gameDetails.alternative_names.map((eachTitle) => {\n            return (\n              <div>\n                <h3>Nombres alternativos:</h3>\n                <p>{eachTitle}</p>\n              </div>\n            );\n          })}\n\n        <h3>Desarrollador(es):</h3>\n        {/* Desarrolladores del juego (vienen en array.) */}\n        {gameDetails.publishers.length !== 0 &&\n          gameDetails.publishers.map((eachPublisher) => {\n            return (\n              <div>\n                <p>{eachPublisher.name}</p>\n              </div>\n            );\n          })}\n\n        <h3>Fecha de salida:</h3>\n        {/* Fecha de release. */}\n        <p>{gameDetails.released}</p>\n\n        <h3>G√©nero(s):</h3>\n        {/* G√©neros del juego que vienen en array */}\n        {gameDetails.genres.length !== 0 &&\n          gameDetails.genres.map((eachGenre) => {\n            return (\n              <div>\n                <p>{eachGenre.name}</p>\n              </div>\n            );\n          })}\n\n        <h3>Consola(s):</h3>\n        {/* Consolas en las que el juego est√° disponible (vienen en array de array) */}\n        {gameDetails.platforms.length !== 0 &&\n          gameDetails.platforms.map((eachPlatform) => {\n            return (\n              <div>\n                <p>{eachPlatform.platform.name}</p>\n              </div>\n            );\n          })}\n\n        <button>\n          <a href={gameDetails.website}>Ir a website</a>\n        </button>\n\n        <h3>Descripci√≥n:</h3>\n        {/* Descripci√≥n del juego */}\n        <Markup content={gameDetails.description} />\n\n        {/* ------------------------------------ */}\n\n        {/* Trailers del juego (vienen en array) */}\n        {gameTrailer.results.length !== 0 &&\n          gameTrailer.results.map((eachTrailer) => {\n            return (\n              <div key={eachTrailer.id}>\n                <h3>Trailers</h3>\n                <video width={900} controls src={eachTrailer.data.max} />\n\n                {/* <source src={eachTrailer.data.max} type=\"video/mp4\" /> */}\n              </div>\n            );\n          })}\n      </div>\n        <div>\n          <h4>Comments:</h4> \n          <Comments />\n        </div>\n    </div>\n  );\n}\n\nexport default VideogamesDetails;\n",[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/auth/Login.jsx",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/auth/Signup.jsx",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/user/Profile.jsx",["115"],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/error/NotFound.jsx",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/pages/error/Error.jsx",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/services/auth.services.js",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/components/Navbar.jsx",["116"],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/services/games.services.js",["117"],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/services/profile.services.js",[],[],"/Users/pelayotrives/Desktop/ironhack-material/modulo3/dia7/gamer-orbit-client/src/services/config.services.js",[],[],"/Users/pelayotrives/Desktop/ironhack-material/mod